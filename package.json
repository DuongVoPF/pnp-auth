{
  "name": "pnp-auth",
  "version": "0.0.3",
  "description": "Provides additional authentication options for @pnp/pnp (aka PnPjs) library",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "npm run tslint && tsc -p .",
    "prepare" : "rimraf -- lib && npm run build",
    "tslint": "tslint --project tsconfig.json",
    "precommit": "npm run tslint",
    "test": "mocha ./test/integration.spec.ts",
    "coverage": "nyc --report-dir coverage mocha ./test/integration.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SharePoint-NodeJS/pnp-auth.git"
  },
  "author": "",
  "contributors": [
    "Sergei Sergeev <sergeev.srg@gmail.com>",
    "Andrew Koltyakov <andrew.koltyakov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SharePoint-NodeJS/pnp-auth/issues"
  },
  "homepage": "https://github.com/SharePoint-NodeJS/pnp-auth",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node-fetch": "^1.6.7",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@pnp/common": "^1.0.1",
    "@pnp/logging": "^1.0.1",
    "@pnp/odata": "^1.0.1",
    "@pnp/sp": "^1.0.1",
    "node-fetch": "^2.0.0",
    "node-sp-auth": "^2.3.3",
    "node-sp-auth-config": "^2.4.3"
  }
}
