{
  "extends": [
    "tslint-config-standard"
  ],
  "rules": {
    "semicolon": [
      true
    ],
    "space-before-function-paren": [
      false
    ],
    "ter-indent": [
      false
    ],
    "no-inferrable-types":true,
    "strict-type-predicates": false,
    "await-promise": false,
    "indent": [
      true,
      "spaces",
      2
    ]
  }
}